      <td><a href="/profile?id={{s.id}}">{{s.id}}</a></td>
      <td><a href="/profile?id={{s.id}}">{{s.name}}</td>
    
	  {% load staticfiles %} 
      <td><a href="/profile?id={{s.id}}"><img src="{{s.InputFile.url}}" class="img-rounded"></td>
      <td><a href="/profile?id={{s.id}}"><img src="{{s.InputFile.url}}" class="img-rounded"></td>
    </tr>
    {% endfor %}
  </tbody>
</table>

